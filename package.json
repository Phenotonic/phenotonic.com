{
  "name": "phenotonic",
  "version": "0.2.0",
  "description": "The website for Phenotonic, an Arizona-based gardening company. Generated with Metalsmith.",
  "engines": {
    "node": "5.9.0"
  },
  "main": "build.js",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "browser-sync": "^2.12.8",
    "css-modularscale": "^1.1.0",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.7.3",
    "metalsmith": "^2.1.0",
    "metalsmith-collections": "^0.7.0",
    "metalsmith-default-values": "0.0.2",
    "metalsmith-drafts": "0.0.1",
    "metalsmith-feed": "^0.2.0",
    "metalsmith-layouts": "^1.6.5",
    "metalsmith-mapsite": "^1.0.4",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-pagination": "^1.4.0",
    "metalsmith-permalinks": "^0.4.0",
    "metalsmith-tags": "^1.2.0",
    "moment": "^2.11.1",
    "nodemon": "^1.9.2",
    "normalize.css": "^4.2.0",
    "postcss": "^5.1.0",
    "postcss-clearfix": "^1.0.0",
    "postcss-cli": "^2.5.2",
    "postcss-color-function": "^2.0.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-extend": "^1.0.4",
    "postcss-fontpath": "^0.3.0",
    "postcss-import": "^8.1.2",
    "postcss-neat": "^2.5.2",
    "postcss-nested": "^1.0.0",
    "postcss-pseudo-class-enter": "^2.0.1",
    "postcss-uncss": "^0.14.0",
    "pug": "^2.0.0-alpha7",
    "standard": "^7.1.2",
    "uglify-js": "^2.7.0",
    "uncss": "^0.14.0"
  },
  "scripts": {
    "start": "npm run browser_sync & NODE_ENV=development nodemon --exec 'npm run build && browser-sync reload'",
    "browser_sync": "browser-sync start --server _build --no-ui --no-notify",
    "build:html": "node build.js",
    "build:css": "postcss -c postcss.json -o _build/css/main.css --no-map.inline css/main.css",
    "build": "npm run build:html && npm run build:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Phenotonic/phenotonic.com.git"
  },
  "keywords": [
    "metalsmith",
    "static",
    "garden",
    "green",
    "sass"
  ],
  "author": "Alec Lomas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Phenotonic/phenotonic.com/issues"
  },
  "homepage": "https://github.com/Phenotonic/phenotonic.com#readme"
}
